    "/i/crashes/resolve": {
      "get": {
        "summary": "Resolve crash",
        "description": "Mark a crash group as resolved",
        "tags": [
           "Crash Analytics"
        ],
        "parameters": [
          {
            "name": "api_key",
            "in": "query",
            "required": true,
            "description": "API key with write access",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "app_id",
            "in": "query",
            "required": true,
            "description": "App ID",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "args",
            "in": "query",
            "required": true,
            "description": "JSON object containing the crash_id",
            "schema": {
              "type": "string",
              "example": "{\"crash_id\":\"CRASH_ID\"}"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Crash resolved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "result": {
                      "type": "string",
                      "example": "Success"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
